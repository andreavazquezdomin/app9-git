<?xml version="1.0" encoding="UTF-8"?>  

<widget xmins ="http://www.w3.org/ns/widgets"
xmlns:gap="http://phonegap.com/ns/1.0"
id = "com.phonegap.app9"
version ="1.0.0">

<name>app9-Reproductor de audio</name>

<description>
Reproductor de audio
</description>

<author href="https://build.phonegap.com" email="candy1324515@gmail.com">
Andrea Vazquez Dominguez
</author>

<!--perzonaliza la aplicacion y plataforma-->
<preference name="orientation" value="default"/>
<!--preference name="orientation" value="portrait"/>
<preference name="orietation" value="landscape"/>-->
<preference name="target-device" value="universal"/>
<preference name="fullscreen" value="true"/>
<preference name="android-installLocation" value="auto"/>
<preference name="permissions" value="INTERNET"/>

<!--Plugins-->
<plugin name="cordova-plugin-device" spec="~1.1.4"/>
<plugin name="cordova-plugin-vibration" spec="~2.1.3"/>
<plugin name="cordova-plugin-dialogs" spec="~1.3.1"/>

<!--Plugin audio-->
<plugin name="cordova-plugin-media" spec="https://github.com/apache/cordova-plugin-media.git"/>
<!--Colocar icono app-->

<icon src="res/android/mdpi.png"/>

</widget>